create table properties(
    height float, velocity float, timeduration float
);
delimiter $$$
create procedure ballsimulation ()
begin
declare g, h, v, t, a float;
set g=9.8;
set t=0.00;
set v=2;
while t<=10.00 do
    set h=10-t;
    set v=0;
    insert into properties values(h,v,t);
    set t=t+0.01;
    set h=0;
    set v=2;
    insert into properties values(h,v,t);
    set t=t+0.01;
end while;
select * from properties;
end $$$

call ballsimulation();
