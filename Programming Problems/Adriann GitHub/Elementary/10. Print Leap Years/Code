delimiter $$$
create procedure leapyear (n int)
begin
declare i int;
declare j int;
set i=year(now());
set j=1;
drop table if exists leapyears;
create table leapyears (
    leapyearlist int);
while j<=n do
    if i%4=0 and i%100<>0 then insert into leapyears values(i);
    set j=j+1;
    end if;
    set i=i+1;
    end while;
select * from leapyears;
end $$$

call leapyear(20);
