select 
    if(a+b>c and b+c>a and a+c>b,
       if(a=b and a=c, 
            "Equilateral", 
            if(a=b or a=c or b=c,
                "Isosceles",
                if(a<>b and b<>c and a<>c,
                    "Scalene",
                    "Not A Triangle"))),
       "Not A Triangle") as triangle_type from triangles
